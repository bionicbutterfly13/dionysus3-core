POSTGRES_DB=agi_db
POSTGRES_USER=agi_user
POSTGRES_PASSWORD=agi_password
DATABASE_URL=postgresql://agi_user:agi_password@localhost:5433/agi_memory

# LLM defaults
LLM_PROVIDER=openai
OPENAI_API_KEY=your_openai_key
OPENAI_MODEL=gpt-5-mini

